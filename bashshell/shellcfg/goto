#!/bin/bash

# echo 'Hello Shell , in goto script'
# /Users/Edison/work/me/src/github
case "${1}" in
	"jump")
		cd  ~/work/projects/Jump/work/Jump
		echo "Now u are @   ==> `pwd`"
		;;
	"jump2")
		cd  ~/work/projects/Jump/backup/Jump
